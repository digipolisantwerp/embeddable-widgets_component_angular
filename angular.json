{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-embeddable-widgets": {
      "root": "projects/ngx-embeddable-widgets",
      "sourceRoot": "projects/ngx-embeddable-widgets/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ngx-embeddable-widgets/tsconfig.lib.json",
            "project": "projects/ngx-embeddable-widgets/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-embeddable-widgets/src/test.ts",
            "tsConfig": "projects/ngx-embeddable-widgets/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-embeddable-widgets/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngx-embeddable-widgets/tsconfig.lib.json",
              "projects/ngx-embeddable-widgets/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "aot-demo": {
      "root": "projects/aot-demo/",
      "sourceRoot": "projects/aot-demo/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/aot-demo",
            "index": "projects/aot-demo/src/index.html",
            "main": "projects/aot-demo/src/main.ts",
            "polyfills": "projects/aot-demo/src/polyfills.ts",
            "tsConfig": "projects/aot-demo/tsconfig.app.json",
            "assets": [
              "projects/aot-demo/src/favicon.ico",
              "projects/aot-demo/src/assets"
            ],
            "styles": [
              "projects/aot-demo/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/aot-demo/src/environments/environment.ts",
                  "with": "projects/aot-demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "aot-demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "aot-demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "aot-demo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/aot-demo/src/test.ts",
            "polyfills": "projects/aot-demo/src/polyfills.ts",
            "tsConfig": "projects/aot-demo/tsconfig.spec.json",
            "karmaConfig": "projects/aot-demo/karma.conf.js",
            "styles": [
              "projects/aot-demo/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "projects/aot-demo/src/favicon.ico",
              "projects/aot-demo/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/aot-demo/tsconfig.app.json",
              "projects/aot-demo/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "aot-demo",
  "schematics": {
    "@schematics/angular:class": {
      "spec": false
    },
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
